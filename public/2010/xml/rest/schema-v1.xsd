  <xsd:element name="model" type="Model"></xsd:element>

  <xsd:complexType name="Model">
    <xsd:complexContent>
      <xsd:extension base="MultipleContentBlobsAssetType">
        <xsd:sequence>
          <xsd:element name="model_type" type="xsd:string"
                       minOccurs="1" maxOccurs="1" nillable="true"></xsd:element>
          <xsd:element name="model_format" type="xsd:string"
                       minOccurs="1" maxOccurs="1" nillable="true"></xsd:element>
          <xsd:element name="environment" type="xsd:string"
                       minOccurs="1" maxOccurs="1" nillable="true"></xsd:element>
