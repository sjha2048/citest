<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.sysmo-db.org/2010/xml/rest"
            xml:lang="en" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sysmo-db.org/2010/xml/rest"
            elementFormDefault="qualified" xmlns:xlink="http://www.w3.org/1999/xlink"
            xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/">


  <xsd:import namespace="http://purl.org/dc/elements/1.1/"
              schemaLocation="dc.xsd"/>
  <xsd:import namespace="http://purl.org/dc/terms/"
              schemaLocation="dcterms.xsd"/>
  <xsd:import namespace="http://www.w3.org/1999/xlink"
              schemaLocation="xlink.xsd"/>


  <xsd:complexType name="ResourceLink">
    <xsd:attribute ref="xlink:href" use="required"></xsd:attribute>
    <xsd:attribute ref="xlink:title" use="optional"></xsd:attribute>
    <xsd:attribute name="resourceType" type="ResourceType"
                   use="required"></xsd:attribute>
		<xsd:attribute name="id" type="xsd:positiveInteger" use="optional"></xsd:attribute>
    <xsd:attribute name="uuid" type="xsd:string" use="optional"></xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="IndexLinks">
    <xsd:attribute ref="xlink:href" use="required"></xsd:attribute>
    <xsd:attribute ref="xlink:title" use="optional"></xsd:attribute>
    <xsd:attribute name="resourceType" type="ResourceType"
                   use="required"></xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="MinimalIndex">
    <xsd:complexContent>
      <xsd:extension base="IndexLinks">

      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="MinimalType">
    <xsd:complexContent>
      <xsd:extension base="ResourceLink">
        <xsd:sequence>
          <xsd:element name="id" type="xsd:positiveInteger"
                       minOccurs="1" maxOccurs="1">
          </xsd:element>
          <xsd:element ref="dc:title" maxOccurs="1" minOccurs="1">
          </xsd:element>
          <xsd:element ref="dc:description" maxOccurs="1"
                       minOccurs="0">
          </xsd:element>
          <xsd:element ref="dcterms:created" maxOccurs="1"
                       minOccurs="1">
          </xsd:element>
          <xsd:element ref="dcterms:modified" maxOccurs="1"
                       minOccurs="1">
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


  <xsd:complexType name="AssetType">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="submitter" type="Submitter"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="organisms" type="OrganismItems"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="creators" type="Creators" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="version" type="xsd:string"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="revision_comments" type="xsd:string"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="latest_version" type="ResourceLink"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="versions" type="VersionLinks"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="policy" type="xsd:anyType" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="blob" type="Blob" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="tags" type="Tags" maxOccurs="1"
                       minOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="MultipleContentBlobsAssetType">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="submitter" type="Submitter"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="organisms" type="OrganismItems"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="creators" type="Creators" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="version" type="xsd:string"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="revision_comments" type="xsd:string"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="latest_version" type="ResourceLink"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="versions" type="VersionLinks"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="policy" type="xsd:anyType" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="blobs" type="Blobs" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="tags" type="Tags" maxOccurs="1"
                       minOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="Blobs">
    <xsd:sequence>
      <xsd:element name="blob" type="Blob" minOccurs="0"
                   maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Blob">
    <xsd:complexContent>
      <xsd:extension base="ResourceLink">
        <xsd:sequence>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="md5sum" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="url" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="original_filename" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="content_type" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="is_remote" type="xsd:boolean"
                       minOccurs="1" maxOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="Asset">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="resource" type="ResourceLink"
                       maxOccurs="1" minOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="VersionLinks">
    <xsd:sequence>
      <xsd:element name="version" type="ResourceLink"
                   minOccurs="1" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Submitter">
    <xsd:sequence>
      <xsd:element name="person" type="PersonItem" maxOccurs="1"
                   minOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Creators">
    <xsd:sequence>
      <xsd:element name="person" type="PersonItem" minOccurs="0"
                   maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SimplePersonItem">
     <xsd:sequence>
       <xsd:element ref="dc:title" minOccurs="1" maxOccurs="1"/>
       <xsd:element name="first_name" type="xsd:string"
                              minOccurs="1" maxOccurs="1"/>
       <xsd:element name="last_name" type="xsd:string"
                              minOccurs="1" maxOccurs="1"/>
     </xsd:sequence>
   </xsd:complexType>


  <xsd:complexType name="PublicationCreators">
      <xsd:sequence>
        <xsd:element name="person" type="SimplePersonItem" minOccurs="0"
                           maxOccurs="unbounded"></xsd:element>

      </xsd:sequence>
  </xsd:complexType>


  <xsd:complexType name="AssetItem">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="investigations" type="Investigations"></xsd:element>
  <xsd:element name="investigation" type="Investigation"></xsd:element>

  <xsd:complexType name="Investigation">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
                    <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
                    <xsd:element name="submitter" type="Submitter"
						minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="creators" type="Creators" minOccurs="1"
                       maxOccurs="1"></xsd:element>
                    <xsd:element name="policy" type="xsd:anyType" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="associated" type="AssociatedItems"
                       minOccurs="1" maxOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="Investigations">
    <xsd:complexContent>
      <xsd:extension base="MinimalIndex">
        <xsd:sequence>
          <xsd:element name="parameters" type="Parameters"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="statistics" type="PagedStatistics"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="items" type="InvestigationItems"
                       maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="related" type="xsd:anyType"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="InvestigationItems">
    <xsd:sequence>
      <xsd:element name="investigation" type="InvestigationItem"
                   maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="InvestigationItem">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
                    <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="studies" type="Studies"></xsd:element>
  <xsd:element name="study" type="Study"></xsd:element>

  <xsd:complexType name="Study">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="submitter" type="Submitter"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="creators" type="Creators" minOccurs="1"
                       maxOccurs="1"></xsd:element>
                    <xsd:element name="policy" type="xsd:anyType" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="associated" type="AssociatedItems"
                       minOccurs="1" maxOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="Studies">
    <xsd:complexContent>
      <xsd:extension base="MinimalIndex">
        <xsd:sequence>
          <xsd:element name="parameters" type="Parameters"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="statistics" type="PagedStatistics"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="items" type="StudyItems" maxOccurs="1"
                       minOccurs="1"></xsd:element>
          <xsd:element name="related" type="xsd:anyType"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="StudyItems">
    <xsd:sequence>
      <xsd:element name="study" type="StudyItem" maxOccurs="unbounded"
                   minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="StudyItem">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="assays" type="Assays"></xsd:element>
  <xsd:element name="assay" type="Assay"></xsd:element>

  <xsd:complexType name="Assay">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="submitter" type="Submitter"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="organisms" type="OrganismItems"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="creators" type="Creators" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="policy" type="xsd:anyType" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="tags" type="Tags" maxOccurs="1"
                       minOccurs="1"></xsd:element>
          <xsd:element name="assay_class" type="ResourceLink"
                       maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="assay_type" type="ResourceLink"
                       maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:choice>
            <xsd:element name="technology_type" type="ResourceLink" maxOccurs="1" minOccurs="1"></xsd:element>
            <xsd:element name="technology_type" maxOccurs="1" minOccurs="1" nillable="true"></xsd:element>
          </xsd:choice>
          <xsd:element name="assay_organisms" type="AssayOrganisms"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="data_relationships" type="DataRelationships"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="associated" type="AssociatedItems"
                       minOccurs="1" maxOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="DataRelationships">
    <xsd:sequence>
      <xsd:element name="data_relationship" type="DataRelationship"
                   maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DataRelationship">
    <xsd:sequence>
      <xsd:element name="data_file" type="ResourceLink"
                   maxOccurs="1" minOccurs="1">
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" type="xsd:string" use="required"></xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="AssayOrganisms">
    <xsd:sequence>
      <xsd:element name="assay_organism" type="AssayOrganism"
                   maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="AssayOrganism">
    <xsd:sequence>
      <xsd:element name="organism" type="MinimalType"
                   maxOccurs="1" minOccurs="1"></xsd:element>
      <xsd:element name="strain" type="MinimalType" maxOccurs="1"
                   minOccurs="0"></xsd:element>
      <xsd:element name="culture_growth" type="ResourceLink"
                   maxOccurs="1" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Assays">
    <xsd:complexContent>
      <xsd:extension base="MinimalIndex">
        <xsd:sequence>
          <xsd:element name="parameters" type="Parameters"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="statistics" type="PagedStatistics"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="items" type="AssayItems" maxOccurs="1"
                       minOccurs="1"></xsd:element>
          <xsd:element name="related" type="xsd:anyType"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AssayItems">
    <xsd:sequence>
      <xsd:element name="assay" type="AssayItem" maxOccurs="unbounded"
                   minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="AssayItem">
    <xsd:complexContent>
      <xsd:extension base="AssetItem">
        <xsd:sequence>
          <xsd:element name="assay_class" type="ResourceLink"
                       maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="assay_type" type="ResourceLink"
                       maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:choice>
            <xsd:element name="technology_type" type="ResourceLink" maxOccurs="1" minOccurs="1"></xsd:element>
            <xsd:element name="technology_type" maxOccurs="1" minOccurs="1" nillable="true"></xsd:element>
          </xsd:choice>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="specimens" type="Specimens"></xsd:element>
  <xsd:element name="specimen" type="Specimen"></xsd:element>

  <xsd:complexType name="Specimen">
