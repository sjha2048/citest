          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="submitter" type="Submitter"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="organisms" type="OrganismItems"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="creators" type="Creators" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="policy" type="xsd:anyType" minOccurs="0" maxOccurs="1"></xsd:element>

          <xsd:element name="associated" type="AssociatedItems"
                       minOccurs="1" maxOccurs="1"></xsd:element>

        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="Specimens">
    <xsd:complexContent>
      <xsd:extension base="MinimalIndex">
        <xsd:sequence>
          <xsd:element name="parameters" type="Parameters"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="statistics" type="PagedStatistics"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="items" type="SpecimenItems" maxOccurs="1"
                       minOccurs="1"></xsd:element>
          <xsd:element name="related" type="xsd:anyType"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
