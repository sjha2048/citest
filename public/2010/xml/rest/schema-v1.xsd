                       minOccurs="0" maxOccurs="1">
          </xsd:element>
          <xsd:element name="items" type="ModelItems" maxOccurs="1"
                       minOccurs="1"></xsd:element>
          <xsd:element name="related" type="xsd:anyType"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

